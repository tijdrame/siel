/*
onglets principaux global nav & menus deroulant
(Partie sensible - non compatible IE - à modifier avec précaution)
*/

/*Definission menu horizontal - les onglets*/

#globalNav{
background-color: transparent;
padding: 0em 0em 0em 2em;
white-space: nowrap;
list-style: none;
margin: 0;
height: auto;
line-height: normal;
list-style-image: none;
list-style-type: none;
}

#globalNav>li{
display: inline;
}

#globalNav li{
position:relative;
}

#globalNav li a{
/*background-color: transparent;*/
border-color: #8cacbb;
border-width: 1px;
border-style: solid solid solid solid;
color: #436976;
margin-right: 0.5em;
padding: 0 1em 0 1em;
text-decoration: none;
text-transform: none;
overflow:hidden;
text-align:center;
}

/*onglet selectionné & hover*/
#globalNav>li:hover>a, #globalNav>li.selected>a{
	background: transparent  url(../img/grad.gif) repeat-x;
	/*border-style: solid solid none solid;*/
}

/*
Definition du menu vertical
*/

/*Definition commune à l'ensemble des niveaux du menu vertical*/
#globalNav li:hover>ul {
display:block; 
position:absolute; 
top:-1px; 
left:151px;
width:151px; 
margin: 0;
padding: 0;
background: #E9EFF5  url(../img/grad.gif) repeat-x;
border:#8cacbb 1px solid;
z-index:150;
list-style: none;
}

/*Partcularité de positionnement du 1er Niveau du menu vertical */
#globalNav > li:hover > ul{
top:13px;
left:0px;
/*border-style: none solid solid solid;*/
}

/*balise li d'un menu vertical*/
#globalNav li ul li{
margin:0;
padding:0;
}

/*balise a d'un menu vertical*/
#globalNav li ul li a{
display:block;
border-style:none;
margin:0;
border-width: 0px;
border-style: none;
background-color: transparent;
color: #436976;
text-align:left;
line-height:24px;
}


/*element a:hover d'un menu vertical*/
#globalNav li ul li:hover> a{
	background-color: #8CACBB;
	font-weight: bold;
	color: White;
}

/*element a:hover d'un menu vertical avec suite*/
#globalNav li ul li.suite:hover> a{
	background: #8CACBB url(../img/expand_lin.gif) right no-repeat;
}

/*element li d'un menu vertical avec indication d'une suite*/
#globalNav li ul li.suite{
background: transparent url(../img/expand_lin.gif) right no-repeat;
}

/*Par default tous les menus verticaux sont cachés*/ 
#globalNav li ul {display: none;}

/*
Fin gestion des onglets et du menu deroulant
*/

