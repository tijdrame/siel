<?xml version="1.0" encoding="UTF-8"?>
<tns:Configuration hierarchy="fr" description="France"
	xmlns:tns="http://www.example.org/Holiday" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.example.org/Holiday Holiday.xsd ">
	<tns:Holidays>
	</tns:Holidays>
	<tns:SubConfigurations hierarchy="jf"
		description="Jour Fériés Seuls">
		<tns:Holidays>
			<tns:Fixed month="JANUARY" day="1" />
			<tns:Fixed month="MAY" day="1" />
			<tns:Fixed month="MAY" day="8" />
			<tns:Fixed month="JULY" day="14" />
			<tns:Fixed month="AUGUST" day="15" />
			<tns:Fixed month="NOVEMBER" day="1" />
			<tns:Fixed month="NOVEMBER" day="11" />
			<tns:Fixed month="DECEMBER" day="25" />
			<tns:ChristianHoliday type="EASTER_MONDAY" />
			<tns:ChristianHoliday type="ASCENSION_DAY" />
			<tns:ChristianHoliday type="WHIT_MONDAY"
				validFrom="2003" />
			<tns:ChristianHoliday type="WHIT_MONDAY"
				validTo="2008" />
		</tns:Holidays>
	</tns:SubConfigurations>
	<tns:SubConfigurations hierarchy="za"
		description="Vacances Scolaires Zone A">
		<tns:Holidays>
			<tns:FixedWeekdayBetweenFixed>
				<tns:from validFrom="2010" month="DECEMBER" day="20" />
				<tns:to validTo="2010" month="DECEMBER" day="31" />
			</tns:FixedWeekdayBetweenFixed>
			<tns:FixedWeekdayBetweenFixed>
				<tns:from validFrom="2011" month="JANUARY" day="01" />
				<tns:to validTo="2011" month="JANUARY" day="10" />
			</tns:FixedWeekdayBetweenFixed>
		</tns:Holidays>
	</tns:SubConfigurations>




	<tns:SubConfigurations hierarchy="ma"
		description="Martinique">
		<tns:Holidays>
			<tns:Fixed month="MAY" day="22" />
		</tns:Holidays>
	</tns:SubConfigurations>
	<tns:SubConfigurations hierarchy="gu"
		description="Guadeloupe">
		<tns:Holidays>
			<tns:Fixed month="MAY" day="27" />
		</tns:Holidays>
	</tns:SubConfigurations>
	<tns:SubConfigurations hierarchy="gy"
		description="Guyane">
		<tns:Holidays>
			<tns:Fixed month="JUNE" day="10" />
		</tns:Holidays>
	</tns:SubConfigurations>
	<tns:SubConfigurations hierarchy="lr"
		description="La Reunion">
		<tns:Holidays>
			<tns:Fixed month="DECEMBER" day="20" />
		</tns:Holidays>
	</tns:SubConfigurations>
	<tns:SubConfigurations hierarchy="br"
		description="Bas-Rhin">
		<tns:Holidays>
			<tns:Fixed month="DECEMBER" day="26" />
			<tns:ChristianHoliday type="GOOD_FRIDAY" />
		</tns:Holidays>
	</tns:SubConfigurations>
	<tns:SubConfigurations hierarchy="hr"
		description="Haut-Rhin">
		<tns:Holidays>
			<tns:Fixed month="DECEMBER" day="26" />
			<tns:ChristianHoliday type="GOOD_FRIDAY" />
		</tns:Holidays>
	</tns:SubConfigurations>
	<tns:SubConfigurations hierarchy="mo"
		description="Moselle">
		<tns:Holidays>
			<tns:Fixed month="DECEMBER" day="26" />
		</tns:Holidays>
	</tns:SubConfigurations>
</tns:Configuration>
